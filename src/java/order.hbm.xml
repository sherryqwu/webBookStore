<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.me.pojo.Order" table="BookStore.order">
    <id column="id" name="orderid" type="int">
      <generator class="native"/>
    </id>
    <property column="userid" name="userid" type="int" unique="true"/>
    <property column="cartid" name="cartid" type="int" unique="true"/>
    <property column="addressid" name="addressid" type="int" unique="true"/>
    
    

  </class>

</hibernate-mapping>
